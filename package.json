  "bugs": {
    "url": "https://github.com/ARKrew/meetup/issues"
  },
  "homepage": "https://github.com/ARKrew/meetup#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "immutability-helper": "^2.6.4",
    "mongoose": "^5.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-totp": "0.0.2",
    "react-scripts": "^1.1.0"
